<p id="notice"><%= notice %></p>

<h1>Sales Summaries</h1>

<table>
  <thead>
    <tr>
      <th>Tasters</th>
      <th>Sales volume</th>
      <th>Date</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @sales_summaries.each do |sales_summary| %>
      <tr>
        <td><%= sales_summary.tasters %></td>
        <td><%= sales_summary.sales_volume %></td>
        <td><%= sales_summary.date %></td>
        <td><%= link_to 'Show', sales_summary %></td>
        <td><%= link_to 'Edit', edit_sales_summary_path(sales_summary) %></td>
        <td><%= link_to 'Destroy', sales_summary, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Sales Summary', new_sales_summary_path %>
